stack=[]
while True:
  print("1. Push")
  print("2. Pop")
  print("3. Display")
  print("4. Exit")
  opt=int(input("Enter your option"))
  if opt==1:
     num=int(input("Enter any element"))
     stack.append(num)
     print(f'{num} is pushed inside stack')
  elif opt==2:
     if len(stack)==0:
       print("Stack is Empty")
     else:
  ele=stack.pop()
     print(f'{ele} poped from stack')
     elif opt==3:
     print(f'Stack is {stack}')
  elif opt==4:
     break
